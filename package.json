    "koa-bodyparser": "4.4.0",
    "koa-logger": "3.2.1",
    "nunjucks": "3.2.4",
    "semver": "7.6.3",
    "winston": "3.8.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@tsconfig/esm": "1.0.5",
    "@tsconfig/node18": "2.0.0",
    "@tsconfig/recommended": "1.0.2",
    "@tsconfig/strictest": "2.0.1",
    "@types/common-tags": "1.8.1",
