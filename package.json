{
  "name": "@opencastsoftware/govuk-nunjucks-renderer",
  "version": "0.1.0",
  "description": "A renderer for GOV.UK frontend Nunjucks components",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --loader:.node=file --sourcemap --platform=node --target=node18 --outdir=dist",
    "lint": "eslint src/** --ext .ts",
    "test": "jest --detectOpenHandles --runInBand --forceExit",
    "start": "node ./dist/index.js",
    "prettier": "prettier --write src/",
    "prettier-check": "prettier --check src/",
    "dev": "nodemon"
  },
